// Exercise 2.2.2

module tape;
module residential {
	implicit_enduses NONE;
}

clock {
	timezone PST+8PDT;
	starttime '2001-01-01 00:00:00 PST';
	stoptime '2001-02-01 00:00:00 PST';
}

object house {
	name house_1;
	object waterheater {
		name heater_1;
		object multi_recorder {
			property "::clock, temperature[degC], house_1:air_temperature[degC]";
			file "multirecorder.csv";
			interval -1;
			limit 100;
		};
	};
}

