// validates proper calculation of plugs.load property with a variable power factor;
// tape input file is generic_demand_pf.glt;
// plugs.fraction set to 1; based on the following equation:

// (complex, based on power factor) plugs.fraction*plugs.capacity

// payroll: d3y332

clock {
	timezone PST+8PDT;
	timestamp '2000-01-01 0:00:00';
	stoptime '2000-02-01 0:00:00';
}

module commercial;
module tape;
module assert;


object office {
	floor_area 4000 sf;
	floor_height 9 ft;
	exterior_ua 2.0;
	interior_ua 2.0;
	interior_mass 2000;
	heating_setpoint 70;
	cooling_setpoint 89;
	thermostat_deadband 4;
	plugs.capacity 1000;
	plugs.fraction 1;
	object player {
		file ../generic_demand_pf.glt;
		property plugs.power_factor;
	};
	object complex_assert {
		target "plugs.load";
		within .1;
		object player {
			property value;
			file ../load_pf.player;
		};
	};
	object recorder {
		file "commercial_lights_usage.csv";
		interval 3600;
		limit 24;
		property plugs.fraction,plugs.load;
	};
}
