// validates commercial HVAC power factor effect on hvac.load by putting HVAC into AUX mode;
// initial air temperature set to 60 degF,with heating/cooling setpoints set to 70/73 
// with a .1degF deadband lights added to supply internal heating gains occupancy set 
// to 1 for all hours of all seven days of the week; outside temperature is set to 86.0 degF
// by hour-long custom made tmy file generic.tmy2


// OCCUPANCY IS BASED ON OLD SCHEDULING 
// SYSTEM (which doesn't take in the correct input) MAY NEED TO BE UPDATED ONCE THE SWITCH 
// TO THE NEW SCHEDULING SYSTEM IS MADE; BASED ON OUTSIDE OFFICE TEMP OF 59degF;

// payroll: d3y332

clock {
	timestamp '2000-01-01 0:00:00';
	stoptime '2000-02-01 0:00:00';
	timezone PST+8PDT;
}

module commercial;
module tape;
module assert;


object office {
	floor_area 4000 sf;
	floor_height 9 ft;
	exterior_ua 2.0;
	interior_ua 2.0;
	interior_mass 2000;
	heating_setpoint 70;
	cooling_setpoint 73;
	hvac.cooling.cop -3;
	hvac.heating.cop 1.25;
	thermostat_deadband .1;
	air_temperature 60;
	control.ventilation_fraction 0;
	lights.capacity 50;
	lights.fraction 1;
	lights.power_factor 1;
	hvac.power_factor .9;
	schedule "0-6 0-23";
	object complex_assert {
		target "hvac.load";
		within .001;
		value 87.1283+42.1981j; 
		object player {
			file ../office_hvac_ECON_assert.player;
			property status;
			loop 0;
		};
	};
	object recorder {
		file "commercial_hvac_usage2.csv";
		interval 1;
		limit 60;
		property outdoor_temperature,occupancy,control.ventilation_fraction,air_temperature,mass_temperature,hvac_mode,hvac.load;
	};
}